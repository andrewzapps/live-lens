{
  "manifest_version": 3,
  "name": "Live Lens",
  "version": "1.0.0",
  "description": "Describe any part of a webpage with AI vision — get spoken and written answers on demand.",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>",
    "https://api.openai.com/*"
  ],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_title": "Live Lens — describe this page"
  },
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": [
        "assets/index.tsx-loader-DgREjVmL.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_idle",
      "css": [
        "assets/index-ltvwqwFO.css"
      ]
    }
  ],
  "commands": {
    "describe-page": {
      "suggested_key": {
        "default": "Alt+Shift+D",
        "mac": "Alt+Shift+D"
      },
      "description": "Open Live Lens to describe page or selection"
    }
  },
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "assets/client-Cw25wJni.js",
        "assets/index.tsx-CkMhYDPe.js"
      ],
      "use_dynamic_url": false
    }
  ]
}
